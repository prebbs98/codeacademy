// This is my age in human years
const myAge = 20;
// This variable defines the first 2 years of dog years to human years
let earlyYears = 2;
earlyYears *= 10.5;
// This variable defines lateryears
let laterYears = myAge - 2;
// I multiplied the variable laterYears with 4 to account for the later years
laterYears *= 4;
console.log(earlyYears, laterYears);
// ----
myAgeInDogYears = earlyYears + laterYears;
var myName = "Preben Albrektsen" .toLowerCase();
// This outputs my my years in dog years to the console
console.log(`My name is ${myName}. I am ${myAgeInDogYears} years old in dog years`);
